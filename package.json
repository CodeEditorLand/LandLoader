{
	"description": "An universal AMD Loader",
	"devDependencies": {
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5",
		"yaserver": "0.4.0"
	},
	"directories": {
		"test": "tests"
	},
	"main": "index.js",
	"name": "@vscode/loader",
	"scripts": {
		"compile": "tsc -p src/core/tsconfig.json && tsc -p tsconfig.json",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"simpleserver": "yaserver --root ./ --port 9999",
		"watch1": "tsc -w -p src/core/tsconfig.json",
		"watch2": "tsc -w -p tsconfig.json"
	},
	"type": "module"
}
